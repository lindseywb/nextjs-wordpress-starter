    "@svgr/webpack": "^6.2.1",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
